{
  "name": "admin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node build/index.js",
    "dev": "nodemon index.ts",
    "build": "tsc",
    "lint": "eslint --fix src test",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "db:push": "npx dotenv -e .env -- prisma db push",
    "db:seed": "npx dotenv -e .env -- prisma db seed",
    "migrate:dev": "npx dotenv -e .env -- prisma generate dev",
    "migrate:reset": "npx dotenv -e .env -- prisma generate reset",
    "prisma:generate": "npx dotenv -e .env -- prisma generate",
    "prisma:studio": "npx dotenv -e .env -- prisma studio",
    "production:build": "npx prisma generate && npx prisma generate deploy && tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "express": "^4.18.2",
    "prisma": "^5.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
